    "test:e2e:deep-links": "cypress run --spec 'cypress/e2e/deep-links.cy.js'",
    "test:e2e:tier-limits": "cypress run --spec 'cypress/e2e/tier-limits.cy.js'",
    "test:a11y": "cypress run --spec 'cypress/e2e/accessibility.cy.js'",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
